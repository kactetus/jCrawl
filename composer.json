{
    "name": "jeremyam/jcrawl",
    "description": "This is a PHP web crawler and scrapper from the command line interface.",
    "license": "MIT",
    "authors": [
        {
            "name": "jeremyam",
            "email": "jeremy@jeremyam.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "nesbot/carbon": "^1.21",
        "illuminate/database": "^5.2",
        "vlucas/phpdotenv": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Crawler\\": "src/"
        },
        "files": [
            "src/Library/functions.php",
            "src/app/config.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.6-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
